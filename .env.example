# 环境配置
NODE_ENV=production

# 数据库配置（强烈建议修改密码！）
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=CHANGE_THIS_STRONG_PASSWORD_16_CHARS_MIN
DB_NAME=g8_community

# JWT 配置（强烈建议修改密钥！）
JWT_SECRET=CHANGE_THIS_TO_RANDOM_STRING_32_CHARS_OR_MORE
JWT_EXPIRATION=86400

# Nginx 对外端口（HTTP）
HTTP_PORT=80

# CORS 配置（生产环境填写实际域名）
CORS_ORIGIN=http://localhost

# 前端 API 地址（通过 Nginx 反向代理，使用相对路径）
VITE_API_URL=/api
